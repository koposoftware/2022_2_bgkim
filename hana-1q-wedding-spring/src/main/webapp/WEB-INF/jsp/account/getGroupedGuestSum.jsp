	<script>


	let groupedGuestSumNameList = [];

	let groupedGuestSumValueList = [];
		
		fetch("member/getGroupedGuestSum")
		.then(res=>res.json())
		.then(res=>{
			let transactionList = res;
			for(let i=0 ;i<transactionList.length;i++){
				
				groupedGuestSumNameList.push(transactionList[i].GUEST_GROUP);
				groupedGuestSumValueList.push(transactionList[i].COUNT);
			
			}
			
		})
		.then(res=>{
			
			Highcharts.chart('container3', {
			    chart: {
			        type: 'column',
			        options3d: {
			            enabled: true,
			            alpha: 10,
			            beta: 25,
			            depth: 70
			        },
			        height:400,
			        backgroundColor: 'rgba(255,255,255,0.5)',
			        shadow: false
			    },
			    title: {
			        text: `<h3 class="h3 card-title mt-3"
						style="font-family: hanaM; font-size: 35px;text-shadow: 2px 2px 2px #aaa; font-weight: 700">소속별 하객 축의금 총합</h3>`
			    },
			    
			    plotOptions: {
			        column: {
			            depth: 25
			        }
			    },
			    xAxis: {
			        categories: groupedGuestSumNameList,
			        labels: {
			            skew3d: true,
			            style: {
			                fontSize: '16px',
			                color:"#008485"
			            }
			        }
			    },
			    yAxis: {
			        title: {
			            text: '축의금 총합',
			            margin: 20,
			            color: '#008485',
			            fontWeight : 'bold',
			            fontSize: '12px',
			            fontFamily : 'hanaM'
			            
			        },
			        labels: {
			            formatter: function() {
			                return this.value  + '원';
			            },
			         lang:{
			        	 thousandsSep: ','
			         },
			         color : "#008485",
			         
			        }
			        
			        
			    },
			    tooltip: {
			        valueSuffix: '원'
			    },
			    series: [{
			        name: '축의금액',
			        data: groupedGuestSumValueList
			    }]
			});

		})
		



</script>




